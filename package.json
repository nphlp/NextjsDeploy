{
    "name": "nextjs-deploy",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "prepare": "husky || true",
        "dev": "next dev",
        "build": "next build",
        "build:analyze": "ANALYZE=true next build",
        "start": "next start",
        "auto": "pnpm install && pnpm db:setup && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:setup && pnpm dev",
        "auto:start": "pnpm install && pnpm db:setup && pnpm prisma:generate && pnpm prisma:deploy && pnpm fixtures:setup && pnpm build && pnpm start",
        "lint": "eslint .",
        "lint:fix": "eslint . --fix",
        "format": "prettier . --check --log-level warn",
        "format:fix": "prettier . --write --log-level error",
        "type": "tsc --noEmit",
        "checks": "pnpm type && pnpm lint:fix && pnpm format:fix",
        "test:run": "vitest run",
        "test:watch": "vitest",
        "test:coverage": "vitest run --coverage",
        "db:setup": "./scripts/db.sh setup",
        "db:reset": "./scripts/db.sh reset",
        "db:reload": "./scripts/db.sh reload",
        "db:execute": "./scripts/db.sh execute",
        "prisma:generate": "pnpm dlx prisma generate",
        "prisma:migrate": "pnpm dlx prisma migrate dev",
        "prisma:deploy": "pnpm dlx prisma migrate deploy",
        "prisma:reset": "pnpm dlx prisma migrate reset",
        "prisma:studio": "pnpm dlx prisma studio",
        "fixtures:setup": "tsx scripts/fixtures.ts setup",
        "fixtures:reset": "tsx scripts/fixtures.ts reset",
        "fixtures:reload": "tsx scripts/fixtures.ts reload",
        "solid:list": "tsx solid/solid-generator.ts list",
        "solid:clear": "tsx solid/solid-generator.ts clear",
        "solid:all": "tsx solid/solid-generator.ts all"
    },
    "dependencies": {
        "@base-ui/react": "^1.1.0",
        "@prisma/adapter-pg": "^7.3.0",
        "@prisma/client": "^7.3.0",
        "dayjs": "^1.11.19",
        "dotenv": "^17.2.3",
        "next": "16.1.0-canary.13",
        "pg": "^8.17.2",
        "react": "^19.2.3",
        "react-dom": "^19.2.3",
        "tsx": "^4.21.0"
    },
    "devDependencies": {
        "@commitlint/cli": "^20.3.1",
        "@commitlint/config-conventional": "^20.3.1",
        "@commitlint/types": "^20.3.1",
        "@hookform/resolvers": "^5.2.2",
        "@next/bundle-analyzer": "^16.1.4",
        "@orpc/client": "^1.13.4",
        "@orpc/openapi": "^1.13.4",
        "@orpc/server": "^1.13.4",
        "@orpc/zod": "^1.13.4",
        "@react-email/components": "^1.0.6",
        "@react-email/render": "^2.0.4",
        "@scalar/api-reference-react": "^0.8.33",
        "@semantic-release/changelog": "^6.0.3",
        "@semantic-release/git": "^10.0.1",
        "@semantic-release/github": "^12.0.2",
        "@tailwindcss/postcss": "^4.1.18",
        "@trivago/prettier-plugin-sort-imports": "^6.0.2",
        "@types/leaflet": "^1.9.21",
        "@types/lodash": "^4.17.23",
        "@types/node": "^25.0.10",
        "@types/nodemailer": "^7.0.5",
        "@types/react": "^19.2.9",
        "@types/react-dom": "^19.2.3",
        "@vitejs/plugin-react": "^5.1.2",
        "@vitest/coverage-v8": "^4.0.18",
        "babel-plugin-react-compiler": "^1.0.0",
        "better-auth": "^1.4.17",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "embla-carousel-react": "^8.6.0",
        "eslint": "^9.39.2",
        "eslint-config-next": "^16.1.4",
        "eslint-plugin-react-refresh": "^0.4.26",
        "eslint-plugin-unused-imports": "^4.3.0",
        "handlebars": "^4.7.8",
        "husky": "^9.1.7",
        "jsdom": "^27.4.0",
        "leaflet": "^1.9.4",
        "lint-staged": "^16.2.7",
        "lodash": "^4.17.23",
        "lucide-react": "^0.561.0",
        "motion": "^12.29.0",
        "nanoid": "^5.1.6",
        "nodemailer": "^7.0.12",
        "nuqs": "^2.8.6",
        "postcss": "^8.5.6",
        "prettier": "^3.8.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "prisma": "^7.3.0",
        "react-day-picker": "^9.13.0",
        "react-hook-form": "^7.71.1",
        "react-leaflet": "^5.0.0",
        "react-resizable-panels": "^3.0.6",
        "recharts": "^2.15.4",
        "semantic-release": "^25.0.2",
        "server-only": "^0.0.1",
        "tailwind-merge": "^3.4.0",
        "tailwindcss": "^4.1.18",
        "tw-animate-css": "^1.4.0",
        "typescript": "^5.9.3",
        "typescript-cookie": "^1.0.6",
        "vite-tsconfig-paths": "^5.1.4",
        "vitest": "^4.0.18",
        "zod": "^4.3.6",
        "zustand": "^5.0.10"
    },
    "lint-staged": {
        "*.{js,ts,tsx}": "eslint --fix",
        "*.{js,ts,tsx,json,css,md,yml,yaml}": "prettier --write"
    }
}
